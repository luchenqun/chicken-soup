{
  "name": "chicken-soup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "server": "./node_modules/.bin/nodemon ./server/app.js",
    "dev": "./node_modules/.bin/nodemon ./server/app.js | npm run serve"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "date-fns": "^1.29.0",
    "identicon.js": "^2.3.2",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "vue": "^2.5.16",
    "vue-clipboard2": "^0.2.0",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.11.0",
    "vue-timeago": "^4.0.2",
    "vuetify": "^1.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "nodemon": "^1.17.5",
    "vue-template-compiler": "^2.5.16"
  },
  "vue": {
    "devServer": {
      "proxy": {
        "/api": {
          "target": "http://127.0.0.1:8000",
          "changeOrigin": true,
          "pathRewrite": {
            "^/api": "/api"
          }
        }
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "no-unused-vars": [
        0,
        {
          "vars": "all",
          "args": "after-used"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
